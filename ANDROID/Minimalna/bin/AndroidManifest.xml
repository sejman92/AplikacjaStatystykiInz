            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pl.gda.pg.eti.kio.project.footballstatisticmanager.activities.TeamsActivity"
